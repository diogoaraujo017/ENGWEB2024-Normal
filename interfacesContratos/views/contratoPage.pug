
extends layout

block content
    .w3-card-4

        header.w3-container.w3-teal
            h1 Contrato #{contrato._id} 
            a.w3-btn.w3-grey(href="/", style="margin-left: 40px;") Return 
                
        div.w3-container
            ul.w3-ul.w3-card-4(style="width:50%")
                
                li 
                    b Id: 
                    | #{contrato._id}
                li 
                    b Número de Anuncio: 
                    | #{contrato["nAnuncio"]}
                li 
                    b Tipo de Procedimento: 
                    | #{contrato["tipoprocedimento"]}
                li 
                    b Objecto Contrato:  
                    | #{contrato["objectoContrato"]}
                li 
                    b Data de Publicação: 
                    | #{contrato["dataPublicacao"]}
                li 
                    b Data de celebração de contrato: 
                    | #{contrato["dataCelebracaoContrato"]}
                li 
                    b Preço Contratual: 
                    | #{contrato["precoContratual"]}
                li 
                    b Prazo de Execução: 
                    | #{contrato["prazoExecucao"]}
                li 
                    b NIPC: 
                    | #{contrato["NIPC_entidade_comunicante"]}
                li 
                    b Entidade Comunicante: 
                    | #{contrato["entidade_comunicante"]}
                li 
                    b Fundamentação: 
                    | #{contrato["fundamentacao"]}
                
                  
    footer.w3-container.w3-teal
        address 
            span Generated by Diogo Araújo